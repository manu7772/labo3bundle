<div class="col-md-4 col-sm-6">
	{% if menu_Admin.commerce is defined %}
	<div class="ibox float-e-margins">
		<div class="ibox-title">
			<h2><i class="fa {{ menu_Admin.commerce.icon|default(default_icon) }}"></i> {{ menu_Admin.commerce.nom|trans({}, 'labo')|capitalize }}</h2>
		</div>
		<div class="ibox-content no-padding">
			<ul class="list-group">
				{% for item in menu_Admin.commerce.level2 %}
				<li class="list-group-item">
					<!-- <span class="badge badge-primary">16</span> -->
					<i class="fa {{ item.icon|default(default_icon) }}"></i>&nbsp;
					{% if item.path != '#' %}
					<a href="{{ path(item.path) }}">{{ item.nom|trans({}, 'labo')|capitalize }}</a>
					{% else %}
					{{ item.nom|trans({}, 'labo')|capitalize }}
					{% endif %}
				</li>
				{% endfor %}
			</ul>
		</div>
	</div>
	{% else %}
	<div class="alert alert-danger">Menu indisponible</div>
	{% endif %}
</div>